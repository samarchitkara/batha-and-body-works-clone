<script>
  document.querySelector("#change").addEventListener("click", function(){
      window.location.href="payment.html";
  });  

  document.querySelector("#form_ship").addEventListener("submit", bathBody);
  var bathbodyArr = JSON.parse(localStorage.getItem("xyz")) || [];
  function bathBody(event) {
    event.preventDefault();
    var fName = document.querySelector("#fname").value;
    var lName = document.querySelector("#lname").value;
    var add1 = document.querySelector("#add1").value;
    var add2 = document.querySelector("#add2").value;
    var zcode = document.querySelector("#zcode").value;
    var num = document.querySelector("#num").value;

    console.log(fName, lName, add1, add2, zcode, num);
    var bathbodyobj = {
      first_name: fName,
      last_name: lName,
      address1: add1,
      address1: add2,
      zipcode: zcode,
      mobile_no: num,
    }
    console.log(bathbodyobj);
    bathbodyArr.push(bathbodyobj);
    localStorage.setItem("xyz", JSON.stringify(bathbodyArr));

    
  }

</script>
